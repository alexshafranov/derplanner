
domain test
{
    world { a(id64) b(id32) c(vec3) }

    task t1(x, y)
    {
        case( a(x) & b(x) ) -> []
    }

    task t2(x, y)
    {
        case( a(y) & c(y) ) -> []
    }

    task t3(x)
    {
        case( a(x) ) -> []

        case( b(x) ) -> []
    }

    task t4()
    {
        case( a(x) & (x < z) ) -> []
    }
}
